include ../mixin-bullet-list/bullet-list.pug
div.impressions-room
  div.diagram
    div.diagram__value
      span.diagram__number 260
      span.diagram__text голосов
    canvas.diagram__chart
  div.impressions-room__appraisal 
    +bullet-list("Великолепно")(class="bullet-list-block bullet-list-block_marker-color-peach")
    +bullet-list("Хорошо")(class="bullet-list-block bullet-list-block_marker-color-green")
    +bullet-list("Удовлетворительно")(class="bullet-list-block bullet-list-block_marker-color-purple")
    +bullet-list("Разочарован")(class="bullet-list-block bullet-list-block_marker-color-black")